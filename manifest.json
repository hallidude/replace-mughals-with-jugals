{
"manifest_version": 2,
    "name": "Replace Mughals with Jugals",
    "description": "This extension replaces the word Mughals with Jugals",
    "version": "1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
